<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.mirella.pokedex.Item1Activity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastrar Pokemon"
        android:id="@+id/btnCadastrar"
        android:layout_alignParentStart="true"
        android:layout_marginTop="75dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignEnd="@+id/btnCadastrarTipo" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastrar Usuario"
        android:id="@+id/btnCadastrarTipo"
        android:layout_below="@+id/btnCadastrar"
        android:layout_alignParentStart="true" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Consultar"
        android:id="@+id/btnConsultar"
        android:layout_toEndOf="@+id/btnCadastrar"
        android:layout_alignTop="@+id/btnCadastrar"
        android:layout_above="@+id/btnCadastrarTipo" />


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.59"
        android:src="@drawable/brakken_charizard_charmander_charmeleon"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />


</RelativeLayout>




